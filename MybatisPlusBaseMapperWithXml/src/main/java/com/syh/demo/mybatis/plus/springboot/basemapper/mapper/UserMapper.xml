<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.syh.demo.mybatis.plus.springboot.basemapper.mapper.UserMapper">
    <select id="selectByIdAndName" parameterType="com.syh.demo.mybatis.plus.springboot.basemapper.entity.User"
            resultType="com.syh.demo.mybatis.plus.springboot.basemapper.entity.User">
        select *
        from user
        where id = #{id} and name = #{name}
    </select>
</mapper>